<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Carina Demo Tests - Mobile Web Sample (Android)" verbose="1" parallel="tests">

    <parameter name="suiteOwner" value="qpsdemo" />
    <parameter name="provider" value="mcloud"/>
    <parameter name="jenkinsJobName" value="Mobile-Web-Demo-Test" />
    <parameter name="jenkinsJobType" value="web"/>
    <parameter name="capabilities" value="browserName=chrome;platformName=ANDROID;deviceName=ANY" />
    <parameter name="jenkinsEnvironments" value="DEMO" />

    <parameter name="jenkinsMobileDefaultPool" value="ANY" />

    <parameter name="jenkinsRegressionPipeline" value="mobile_regression" />
    <parameter name="jenkinsJobExecutionOrder" value="5" />
    <parameter name="jenkinsJobExecutionMode" value="continue" />
    <parameter name="jenkinsAutoScreenshot" value="true"/>


    <test name="Web mobile test">
        <classes>
            <class name="com.qaprosoft.carina.demo.WebMobileSampleTest" />
        </classes>
    </test>

</suite>
